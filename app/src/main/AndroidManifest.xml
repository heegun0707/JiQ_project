            android:label="@string/app_name" />
        <activity android:name="com.qplus.qplus.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.qplus.qplus.LoadingPageActivity" />
        <activity android:name="com.qplus.qplus.ui.user.UpdatePassActivity" />
        <activity android:name="com.qplus.qplus.ui.user.UpdateInfoActivity" />
        <activity android:name="com.qplus.qplus.ui.home.QRcodeActivity" />

        <activity android:name="com.qplus.qplus.SignActivity" />
        <activity android:name="com.qplus.qplus.CouponActivity"/>
        <activity android:name="com.qplus.qplus.ManagerMainActivity" />
        <activity android:name="com.qplus.qplus.QRScanActivity" />
        <activity android:name="com.qplus.qplus.ui.comunity.UpdateActivity" />

        <activity android:name="com.qplus.qplus.ui.market.MarketBuyActivity" />
        <activity android:name="com.qplus.qplus.ui.market.MarketBuyCompleteActivity" />
        <activity android:name="com.qplus.qplus.ui.market.MarketDonateActivity" />
